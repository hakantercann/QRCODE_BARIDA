<UserControl x:Class="QRCODE_BARIDA.UserControls.LogMenu1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QRCODE_BARIDA.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="720" d:DesignWidth="1015"
             Loaded="UserControl_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="65"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="500"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Border Grid.Row="0" Grid.Column="1" Background="AliceBlue">
            <DataGrid x:Name="LogDataGrid" Background="#FFB19D9D" Foreground="Black" VerticalAlignment="Top" FontFamily="Bahnschrift"
                     SelectedCellsChanged="LogDataGrid_SelectedCellsChanged"/>
        </Border>
        <Border Grid.Row="1" Grid.Column="1" Background="AliceBlue">

        </Border>
    </Grid>
</UserControl>
